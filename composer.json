            "email": "damien.matabon@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.*",
        "friendsofsymfony/oauth-server-bundle": "1.4.*@dev",
        "hwi/oauth-bundle": "0.4.*@dev",
        "facebook/php-sdk-v4": "4.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Redking\\Bundle\\OAuthBundle": "" }
